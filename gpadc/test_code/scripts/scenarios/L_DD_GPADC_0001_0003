#This test case verifies Reading GPADC values generated by Phoenix via the I2C bus. This test case reads all the 17 channles

0001 set -x; insmod $GPADC_MODULE  channels=131071 && exit 0 || exit 1
0002 set -x; rmmod $GPADC_MODULE

# End L_DD_GPADC_0001_0001

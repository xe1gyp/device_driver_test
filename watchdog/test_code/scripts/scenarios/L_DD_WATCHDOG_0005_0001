# Begin L_DD_WATCHDOG_0005_0001

0001	set -x; $TESTROOT/../../utils/scripts/dvfs.opp.sh & ($TESTBIN/$TESTWDTIOCTL &) && sleep 3 && killall $TESTWDTIOCTL && exit 0 || exit 1
0002	set -x; ($TESTBIN/$TESTWDTWRITE &) && sleep 3 && killall $TESTWDTWRITE && exit 0 || exit 1
0003	set -x; sleep 10 && ps -aux && exit 0 || exit 1

# End L_DD_WATCHDOG_0005_0001
